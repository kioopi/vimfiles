snippet f "anonymous function"
function(${1}) {${0}
endsnippet

snippet fn "anonymous function block"
function($1) {
  ${VISUAL}$2
}$0
endsnippet

snippet cl "console.log"
console.log(${1});
endsnippet

snippet cw "console.warn"
console.warn(${1});
endsnippet

snippet ce "console.error"
console.error(${1});
endsnippet
